node {
    stage('SCM') {
    	echo 'Gathering code from version control'
	git branch: "${params.branch}" , url: 'https://github.com/aalmanzaui1/JenkinsGroovy'
    }
    stage('Build') {
        echo 'Building....'
	sh 'dotnet --version'
	echo 'Building new feature'
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}
